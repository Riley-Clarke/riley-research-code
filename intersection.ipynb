    "    if abs(line.v1.x - line.v2.x) > abs(line.v1.y - line.v2.y):\n",
    "        # Sort by x\n",
    "        points = sorted(points, key=lambda v: v.x)\n",
    "    else:\n",
    "        # Sort by y\n",
    "        points = sorted(points, key=lambda v: v.y)\n",
    "    segments = []\n",
    "    for i in range(len(points)-1):\n",
    "        seg = Line()\n",
    "        seg.v1, seg.v2 = points[i], points[i+1]\n",
    "        seg.x = [seg.v1.x, seg.v2.x]\n",
    "        seg.y = [seg.v1.y, seg.v2.y]\n",
    "        segments.append(seg)\n",
    "    return segments\n"
